<div class="card card-cascade col-12 col-md-5 mb-4 mt-2 px-0">

  <!-- Card image -->
  <div class="view view-cascade overlay">
    <img class="card-img-top"
      src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcQtz1UsljuzVpF9sGiB5uU5mKVFDucOlxRl3Ro00hbX_Uq8m36W"
      alt="Card image cap">
    <a href="#!">
      <div class="mask rgba-white-slight"></div>
    </a>
  </div>

  <!-- Card content -->
  <div class="card-body card-body-cascade text-center pb-0">
    <div class="row">
      <div class="col-6">
        <h6 class="card-title"><strong>{{this.title}}</strong></h6>
        <!-- Subtitle -->
        <p class="blue-text pb-2"><strong>Owner: {{owner.username}}</strong></p>

      </div>
      <div class="col-6">
        <p class="card-text">Players: 8/12 </p>

      </div>
    </div>

    <!-- Title -->
    <!-- Text -->

  </div>

</div>